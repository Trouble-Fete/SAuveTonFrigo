{
	"expo": {
		"name": "SauveTonFrigo",
		"slug": "SauveTonFrigo",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./SRC/assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./SRC/assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"expo": {
			"ios": {
				"infoPlist": {
					"NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à votre bibliothèque de photos pour télécharger des images."
				}
			},
			"android": {
				"permissions": ["READ_EXTERNAL_STORAGE"]
			},
			"adaptiveIcon": {
				"foregroundImage": "./SRC/assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "../SauveTonFrigo/SRC/assets/favicon.png"
		},
		"plugins": ["expo-font"]
	},
	"android": {
		"package": "com.anonymous.SauveTonFrigo"
	}
}
