{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"moduleResolution": "node16",
		"module": "Node16",
		"typeRoots": ["./types", "./node_modules/@types"]
	},
	"include": ["SRC", "types"]
}
